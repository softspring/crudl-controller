{
    "name": "softspring/crudl-controller",
    "description": "A CRUD+list helper classes",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=8.1",
        "softspring/events": "^5.2",
        "softspring/doctrine-paginator": "^5.2",
        "softspring/doctrine-query-filters": "^5.2"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "friendsofphp/php-cs-fixer": "^3.40",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5.1",
        "symfony/console": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "symfony/twig-bridge": "^5.4|^6.0|^7.0",
        "symfony/yaml": "^5.4|^6.0|^7.0",
        "twig/twig": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\CrudlController\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\CrudlController\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
