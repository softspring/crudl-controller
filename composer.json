{
    "name": "softspring/crudl-controller",
    "description": "A CRUD+list helper classes",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "softspring/events": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.25",
        "doctrine/orm": "^2.5",
        "symfony/console": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/form": "^5.4 || ^6.0",
        "symfony/framework-bundle": "^5.4 || ^6.0",
        "symfony/twig-bridge": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0",
        "twig/twig": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\CrudlController\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\CrudlController\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "javihgil/doctrine-pagination-bundle": "Paginate doctrine repositories easily."
    },
    "conflict": {
        "javihgil/doctrine-pagination-bundle": "1.0.*"
    },
    "extra": {
    }
}
